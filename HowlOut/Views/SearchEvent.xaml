<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="HowlOut.SearchEvent"
			 xmlns:common="clr-namespace:UXDivers.Artina.Player;assembly=UXDivers.Artina.Player.Common" BackgroundColor="{DynamicResource HowlOutBackground}"
			 xmlns:local="clr-namespace:HowlOut;assembly=HowlOut">
	<ContentView.Content>
		<StackLayout >
				<ListView x:Name="SearchEventList"
				  			HasUnevenRows="false"
							RowHeight="194"
							SeparatorVisibility="None"
							BackgroundColor="Transparent">
					<ListView.ItemTemplate>
				      <DataTemplate>
				        <ViewCell>
						  <local:SearchEventTemplate  /> 
						</ViewCell>
				      </DataTemplate>
				    </ListView.ItemTemplate>
				  </ListView>
		</StackLayout>

	</ContentView.Content>
</ContentView>