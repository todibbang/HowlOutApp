<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="HowlOut.MapView">
	<ContentView.Content>
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Height="*"/>
				<RowDefinition Height="30"/>
			</Grid.RowDefinitions>
			<StackLayout Grid.Row="0" x:Name="mapLayout"> </StackLayout>
			<Button Grid.Row="1" HeightRequest="30" Text="Pick Location" x:Name="selectButton" HorizontalOptions="FillAndExpand"/>
		</Grid>
	</ContentView.Content>
</ContentView>
