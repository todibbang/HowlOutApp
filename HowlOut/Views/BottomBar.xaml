			<!--<Button Grid.Column="0" Text="Explore" Style="{DynamicResource ButtonStyle}" x:Name="knapOne" BackgroundColor="White"></Button>
			<Button Grid.Column="1" Text="Manage" Style="{DynamicResource ButtonStyle}" x:Name="knapTwo" BackgroundColor="White"></Button>-->
