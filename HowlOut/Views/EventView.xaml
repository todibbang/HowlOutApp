<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="HowlOut.EventView"
			 xmlns:common="clr-namespace:UXDivers.Artina.Player;assembly=UXDivers.Artina.Player.Common" BackgroundColor="{DynamicResource HowlOutBackground}"
			 xmlns:local="clr-namespace:HowlOut;assembly=HowlOut">
	<ContentView.Content>
		<Grid RowSpacing="0" ColumnSpacing="0"  x:Name="contentTest">
			<Grid.RowDefinitions>
				<RowDefinition Height="*"/>
			</Grid.RowDefinitions>

			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="*"/>
			</Grid.ColumnDefinitions>


			<StackLayout Orientation="Vertical" VerticalOptions="FillAndExpand" Spacing="0" Padding="0">


				<ScrollView x:Name="HorizontalScrollView">
					<StackLayout x:Name="searchList" Spacing="0" >
					</StackLayout>
				</ScrollView>
			</StackLayout>

			<ActivityIndicator Color="{DynamicResource HowlOut}" IsRunning="true" x:Name="loading"/>
		</Grid>
	</ContentView.Content>
</ContentView>